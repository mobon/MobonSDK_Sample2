<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="sample.sdk.mobon.com.mobonsdk_sample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >


        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- shopping, 광고 리스트 아이템을 노출하는 샘플코드 -->

        <!-- BannerView 샘플코드 -->
        <activity
            android:name=".SampleBannerActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"></activity>

        <!-- NativeAdView 샘플코드 -->
        <activity
            android:name=".SampleNativeActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".SampleWebActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"></activity>

        <!-- MOBONSDKACTIVITY 비상속 샘플코드 -->

        <!-- 전면 광고 샘플코드 -->
        <activity
            android:name=".SampleInterstitialActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"/>
        
         <!-- NativeAdView 샘플코드 -->
        <activity
            android:name=".SampleNativeActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"></activity>

        <!-- 광고 데이터 처리 샘플코드 -->
        <activity
            android:name=".SampleJsonDataActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"/>


        <!-- [START] MobonSDK 추가 부분 -->
        <!--<meta-data-->
            <!--android:name="com.mobon.sdk.MediaCode"-->
            <!--android:value="YOUR_MEDIA_CODE" />-->

    </application>

</manifest>
